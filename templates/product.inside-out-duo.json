        "text_a8BcyY": {
          "type": "text",
          "settings": {
            "text": "<p><strong>WHAT IT IS</strong></p><p>Hyaluronic acid is a natural molecule shown to ease vaginal atrophy by deeply hydrating, improving elasticity, and soothing dryness.</p>",
            "text_size": "text-body",
            "background_style": "solid",
